{
  "name": "electron-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "main": "src/electron.js",
  "scripts": {
    "start": "concurrently \"yarn react-start\" \"wait-on http://localhost:3000 && yarn electron-start\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "electron-start": "electron .",
    "server": "nodemon server.js --watch server.js --exec babel-node --presets env"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "concurrently": "^3.5.1",
    "electron": "^1.8.2",
    "nodemon": "^1.17.1",
    "wait-on": "^2.1.0"
  },
  "proxy": "http://localhost:8080"
}
