{
  "name": "dispatch-utilities",
  "version": "0.1.0",
  "private": true,
  "main": "./src/client/public/electron.js",
  "scripts": {
    "start":
      "concurrently \"yarn server\" \"wait-on http://localhost:8080 && yarn react-start\" \"wait-on http://localhost:3000 && yarn electron-dev\"",
    "react-start":
      "cross-env BROWSER=none node -r dotenv/config ./src/client/scripts/start.js",
    "server":
      "cross-env NODE_ENV=development nodemon ./src/server/index.js --watch ./src/server/index.js --watch ./src/server/request.js --exec babel-node --presets env --plugins transform-object-rest-spread",
    "build": "node ./src/client/scripts/build.js",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "electron-build":
      "electron-builder --win --x64 -c.extraMetadata.main=build/electron.js",
    "preelectron-build": "yarn build"
  },
  "build": {
    "appId": "com.dispatch-utilities",
    "productName": "Dispatch Utilities",
    "files": ["build/**/*", "node_modules/**/*"],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "^5.0.1",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "node-fetch": "^2.1.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.1",
    "shortid": "^2.2.8",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "electron": "^1.8.2",
    "electron-builder": "^20.4.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "sass-loader": "^6.0.7",
    "wait-on": "^2.1.0"
  },
  "homepage": "./",
  "proxy": "http://localhost:8080",
  "babel": {
    "presets": ["react-app"]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
